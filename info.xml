<?xml version="1.0"?>
<extension key="de.systopia.rebook" type="module">
  <file>rebook</file>
  <name>Rebook Feature</name>
  <description>Allows you to rebook contributions to other contacts</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.rebook</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.rebook</url>
    <url desc="Support">https://github.com/systopia/de.systopia.rebook/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate/>
  <version>1.3-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments>Feature extracted from de.systopia.donrec extension</comments>
  <civix>
    <namespace>CRM/Rebook</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Rebook_Upgrader</upgrader>
</extension>
